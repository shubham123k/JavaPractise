package com.techouts.training;

public class Continue_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         for(int num=0;num<=5;num++) {
        	 if(num!=3) {
        		 System.out.print(num+"    ");
        		 continue;
        	 }
        	 else {
        		 System.out.print("GRANDSTREAM"+"  ");
        	 }
        	 
        	 System.out.print(num+"   ");
         }
	}

}
