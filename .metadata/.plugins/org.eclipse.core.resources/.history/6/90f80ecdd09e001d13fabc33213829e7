package com.techouts.training;

public class Continue_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         for(int num=0;num<=5;num++) {
        	    System.out.println(num);
        	 if(num==3) {
        		// System.out.println(num);
        		 continue;
        	 }
         }
	}

}
